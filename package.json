  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "ava": {
    "files": [
      "**/test/**/*",
      "!week5/test",
      "!**/lecture/test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nadgerz/react-boilerplate.git"
