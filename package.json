{
    "name": "react-boilerplate",
    "version": "0.1.0",
    "description": "Modern JavaScript Tooling with React",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config webpack.config.prod.js",
        "dev": "webpack-dev-server --config webpack.config.dev.js",
        "dev:hot": "webpack-dev-server --hot --config webpack.config.dev.js",
        "format": "prettier --write \"src/**/*.{js,jsx,css,htm,json,ts,html,sass,scss}\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "react",
        "git",
        "javascript",
        "npm",
        "js",
        "webpack",
        "eslint",
        "husky",
        "jest"
    ],
    "author": "Stephen A. Ingram <stephen.a.ingram@gmail.com> (https://moolisoft.com/)",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "7.5.5",
        "@babel/core": "7.5.5",
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "@babel/preset-env": "7.5.5",
        "@babel/preset-react": "7.0.0",
        "babel-loader": "8.0.6",
        "css-loader": "3.2.0",
        "html-webpack-plugin": "3.2.0",
        "prettier": "1.18.2",
        "style-loader": "1.0.0",
        "webpack": "4.39.3",
        "webpack-cli": "3.3.7",
        "webpack-dev-server": "3.8.0",
        "webpack-merge": "4.2.2"
    },
    "dependencies": {
        "@hot-loader/react-dom": "16.9.0",
        "prop-types": "15.7.2",
        "react": "16.9.0",
        "react-dom": "16.9.0",
        "react-hot-loader": "4.12.12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nadgerz/react-boilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/nadgerz/react-boilerplate/issues"
    },
    "homepage": "https://github.com/nadgerz/react-boilerplate#readme"
}
