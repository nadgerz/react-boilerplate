{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Modern JavaScript Tooling with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "format": "pretty-quick --pattern \"**/*.*(js|jsx|scss|sass|css|json|ts|html)\"",
    "format:full": "prettier --write \"./**/*.{js,jsx,css,htm,json,ts,html,sass,scss}\"",
    "lint": "lint-staged",
    "prod": "npm run build",
    "start": "node ./server-start.js",
    "test": "npm run test:jest",
    "test:ava": "ava --verbose",
    "test:ava:watch": "ava --watch",
    "test:coverage": "nyc ava --verbose",
    "test:jest": "jest",
    "test:mocha": "nodemon --exec 'mocha --exit'",
    "test:mocha:min": "nodemon --exec 'mocha -R min --exit'"
  },
  "keywords": [
    "react",
    "git",
    "javascript",
    "npm",
    "js",
    "webpack",
    "eslint",
    "husky",
    "jest"
  ],
  "author": "Stephen A. Ingram <stephen.a.ingram@gmail.com> (https://moolisoft.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@hapi/joi": "17.1.1",
    "@prettier/plugin-php": "0.14.0",
    "@prettier/plugin-ruby": "0.18.1",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.3",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "ava": "3.8.0",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "commitlint": "8.3.5",
    "css-loader": "3.5.3",
    "envinfo": "7.5.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-graphql": "3.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "faker": "4.1.0",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "intel": "1.2.0",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "mocha": "7.1.2",
    "mongodb-memory-server": "6.5.2",
    "nconf": "0.10.0",
    "nodemon": "2.0.3",
    "nyc": "15.0.1",
    "parcel": "1.12.4",
    "prettier": "2.0.5",
    "prettier-plugin-java": "0.7.1",
    "pretty-quick": "2.0.1",
    "progress-estimator": "0.2.2",
