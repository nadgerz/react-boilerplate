{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Modern JavaScript Tooling with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "format": "pretty-quick --pattern \"**/*.*(css|htm|html|js|json|jsx|sass|scss|ts|vue)\"",
    "format:full": "prettier --write \"./**/*.{css,htm,html,js,json,jsx,sass,scss,ts,vue}\"",
    "lint": "lint-staged",
    "prod": "npm run build",
    "start": "node ./server-start.js",
    "test": "npm run test:jest",
    "test:ava": "ava --verbose",
    "test:ava:watch": "ava --watch",
    "test:coverage": "nyc ava --verbose",
    "test:jest": "jest",
    "test:mocha": "nodemon --exec 'mocha --exit'",
    "test:mocha:min": "nodemon --exec 'mocha -R min --exit'"
  },
  "keywords": [
    "react",
    "git",
    "javascript",
    "npm",
    "js",
    "webpack",
    "eslint",
    "husky",
    "jest"
  ],
  "author": "Stephen A. Ingram <stephen.a.ingram@gmail.com> (https://moolisoft.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/plugin-proposal-private-methods": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@hapi/joi": "17.1.1",
    "@prettier/plugin-php": "0.14.3",
    "@prettier/plugin-ruby": "0.20.1",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "ava": "3.12.1",
    "axios": "0.20.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "commitlint": "11.0.0",
    "css-loader": "4.3.0",
    "envinfo": "7.7.3",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "faker": "5.1.0",
    "html-webpack-plugin": "4.4.1",
    "husky": "4.3.0",
    "intel": "1.2.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "mocha": "8.1.3",
    "mongodb-memory-server": "6.7.5",
    "nconf": "0.10.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "parcel": "1.12.4",
    "prettier": "2.1.1",
    "prettier-plugin-java": "0.8.2",
