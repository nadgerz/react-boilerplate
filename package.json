{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Modern JavaScript Tooling with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "format": "pretty-quick --pattern \"**/*.*(js|jsx|scss|sass|css|json|ts|html)\"",
    "format:full": "prettier --write \"./**/*.{js,jsx,css,htm,json,ts,html,sass,scss}\"",
    "lint": "lint-staged",
    "prod": "npm run build",
    "start": "node ./server-start.js",
    "test": "npm run test:jest",
    "test:ava": "ava --verbose",
    "test:ava:watch": "ava --watch",
    "test:coverage": "nyc ava --verbose",
    "test:jest": "jest",
    "test:mocha": "nodemon --exec 'mocha --exit'",
    "test:mocha:min": "nodemon --exec 'mocha -R min --exit'"
  },
  "keywords": [
    "react",
    "git",
    "javascript",
    "npm",
    "js",
    "webpack",
    "eslint",
    "husky",
    "jest"
  ],
  "author": "Stephen A. Ingram <stephen.a.ingram@gmail.com> (https://moolisoft.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-private-methods": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@prettier/plugin-php": "0.11.2",
    "@prettier/plugin-ruby": "0.16.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.3",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "css-loader": "3.3.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-graphql": "3.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "nodemon": "2.0.2",
    "nyc": "14.1.1",
    "parcel": "1.12.4",
    "prettier": "1.19.1",
    "prettier-plugin-java": "0.5.1",
    "pretty-quick": "2.0.1",
    "react-axe": "3.3.0",
    "style-loader": "1.0.1",
    "superagent": "5.1.2",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "@hot-loader/react-dom": "16.11.0",
    "@reach/router": "1.2.1",
    "axios": "0.19.0",
    "chalk": "3.0.0",
    "core-js": "3.4.8",
    "faker": "4.1.0",
    "lodash": "4.17.15",
    "mongodb": "3.4.0",
    "mongoose": "5.8.0",
    "morgan": "1.9.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.18",
    "regenerator-runtime": "0.13.3",
    "uuid": "3.3.3",
    "validator": "12.1.0"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm run test"
    }
  },
  "ava": {
    "files": [
      "**/test/**/*",
      "!week5/test",
      "!**/lecture/test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nadgerz/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/nadgerz/react-boilerplate/issues"
  },
  "homepage": "https://github.com/nadgerz/react-boilerplate#readme"
}
